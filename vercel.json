{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "15mb",
          "includeFiles": [
            "uploads/**",
            "models/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      }
    ]
  }